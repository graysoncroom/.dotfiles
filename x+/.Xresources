URxvt*scrollBar: false
URxvt*urlLauncher: /bin/firefox

! run "fc-list" for a list of available fonts
!URxvt*font: xft:Terminus:pixelsize=11
URxvt*font: xft:bitstream vera sans mono:pixelsize=14:medium
URxvt*boldFont: xft:bitstream vera sans mono:pixelsize=14:bold
URxvt*letterSpace: -2
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800
URxvt*transparent: true
URxvt*shading: 30

URxvt.perl-ext-common:  ...,font-size
URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal
